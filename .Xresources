#include "/home/zkauff/.xcolors/pastel"

! Fonts
Xft.dpi: 96
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

*.font: xft:Iosevka:style=Regular:size=8
*.boldFont: xft:Iosevka:style=Bold:size=8
*.italicFont: xft:Iosevka:style=Italic:size=8
*.boldItalicFont: xft:Iosevka:style=Bold Italic:size=8
rofi.terminal:	urxvtc
rofi.ssh-client:	ssh
rofi.ssh-command:	{terminal} -e {ssh-client} {host}


URxvt.url-launcher:	firefox
URxvt.matcher.button:	1
URxvt.perl-ext-common:	default, tabbed, font
URxvt.tabbed.tabbar-fg:	11
URxvt.tabbed.tabbar-bg:	15
URxvt.tabbed.tab-fg:	11
URxvt.tabbed.tab-bg:	15
URxvt*letterSpace:	-2
URxvt*lineSpace:	-3
URxvt*scrollBar:	false
URxvt*transparent:	false
URxvt*internalBorder: 2
URXvt*externalBorder:	5
URxvt*borderLess:	false
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveLines:	9999
URxvt.iso14755:	false
twobwm.enable_compton:	true

URxvt.iso14755_52: false
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
